{
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "globals": {
      "var1": "writable",
      "var2": "readonly",
      "logger": true
    },
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "camelcase": 0,
      "no-param-reassign": 0,
      "no-shadow": ["error", { "allow": ["req", "res", "err", "next"] }],
      "linebreak-style": 0,
      "no-underscore-dangle": 0,
      "object-curly-newline": 0,
      "arrow-parens": 0
    }
  }
  